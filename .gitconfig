[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    st = status
    co = checkout
    br = branch
    ci = commit
    dft = difftool
    push-to-gerrit = push origin master:refs/for/master
    df = diff --stat
[diff]
    tool = vimdiff
[difftool]
    prompt = No
